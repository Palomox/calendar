{
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://calendarium-api.hopoke.workers.dev/api/:match*"
    },
    {
      "source": "/.ory/:path*",
      "destination": "/api/handler.ts"
    },
    {
      "source": "/((?!.ory/.*).*)",
      "destination": "/index.html"
    }
  ]
}


